{
  "manifest_version": 3,
  "name": "Twitch Mod Assitant",
  "description": "A chrome extension to help Twitch Moderators find bots and other things.",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo1_bot_assitant_34.png"
  },
  "icons": {
    "128": "logo1_bot_assitant_128.png"
  },
  "permissions": [
    "tabs"
  ],
  "devtools_page": "devtools.html",
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "logo1_bot_assitant_128.png",
        "logo1_bot_assitant_34.png"
      ],
      "matches": []
    }
  ]
}